<Window x:Class="LearningWPF.Button10_Dockpanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LearningWPF"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="720" Width="420"
        ResizeMode="NoResize">
    <DockPanel>
        <Button DockPanel.Dock="Top">We're rats, we're rats!</Button>
        <Button DockPanel.Dock="Top">We stalk at night</Button>
        <Button DockPanel.Dock="Left">{rat}</Button>
        <Button DockPanel.Dock="Right">{rat}</Button>
        <Button DockPanel.Dock="Bottom">We're the rats!</Button>
        <Button>We pray at night</Button>
    </DockPanel>
</Window>
